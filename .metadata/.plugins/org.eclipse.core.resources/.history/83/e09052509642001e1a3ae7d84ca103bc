package pratice;

public class MainClassShape {

	public static void main(String[] args) {
		Circle c = new Circle(23);
		Rectangle r = new Rectangle(5,6);
		System.out.println("Circle Area: "+c.area());
		System.out.println("Circle Parameter: "+c.parameter());
		System.out.println("=====================================");
		System.out.println("Rectangle Area: "+c.area());
		System.out.println("Rectangle Parameter : "+c.area());
		

	}

}
