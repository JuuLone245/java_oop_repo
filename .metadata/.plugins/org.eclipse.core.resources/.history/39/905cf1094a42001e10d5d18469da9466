package interfaceoop;

public class Circle implements Shape {
	
	public double radius;
	public Circle(double radius)
	{
		this.radius = radius;
	}
	@Override
	public double parameter() {
	
		return Math.PI*radius*radius;
	}

	@Override
	public double area() {
		// TODO Auto-generated method stub
		return 0;
	}

}
