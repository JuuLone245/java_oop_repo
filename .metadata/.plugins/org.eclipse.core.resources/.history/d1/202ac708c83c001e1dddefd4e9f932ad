package javaoop;

public class Super_class {
	int num = 20;
	public void display()
	{
		System.out.println("This is the display method of superclass");
	}
}
class Sub_class extends Super_class{
	int num=10;
	public void display()
	{
		System.out.println("This is the display method of subclass");
	}
}
